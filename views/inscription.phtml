        <section class="section2">
            <article class="gauche">
                
                <div class="champcheck">
                    <label>Civilité:</label>
                    <div>
                    <label for="homme">Mr.</label>
                        <input type="checkbox" name="homme" class="checkbox">
                    <label for="femme">Mme.</label>
                        <input type="checkbox" name="femme" class="checkbox">
                    </div>
                </div>

                <div class="champ">
                    <label for="nom">Nom:</label>
                        <input type="text" name="nom" required placeholder="Nom">
                </div>

                <div class="champ"> 
                    <label for="prenom">Prénom:</label>   
                        <input type="text" name="prenom" required placeholder="Prénom">
                </div>

                <div class="champ">
                    <label for="reference">Référence vendeur:</label>    
                        <input type="text" name="reference" required placeholder="Numéro de référence du vendeur">
                </div>

                <div class="champ">
                    <label for="phone">Téléphone:</label> 
                        <input type="text" name="phone" required placeholder="Téléphone">
                </div>

                <div class="champ">
                    <label for="mail">E-mail:</label>
                        <input type="mail" name="mail" required placeholder="E-mail">   
                </div>

            </article>
            
            <article class="droite">

                <div class="champ">
                    <label for="password">Mot de passe:</label>    
                        <input type="text" name="password" required placeholder="Password">
                </div>

                <div class="champ">
                    <label for="nomSociete">Nom de la société:</label>  
                        <input type="text" name="nomSociete" required placeholder="Nom de la société">
                </div>

                <div class="champ">
                    <label for="adresse">Adresse de la société:</label>    
                        <input type="text" name="adresse" required placeholder="Adresse de la société">
                </div>

                <div class="champ"> 
                    <label for="phoneSociete">Téléphone de la société:</label>  
                        <input type="text" name="phoneSociete" required placeholder="Téléphone de la société">
                </div>
                <div class="checknews">
                <input type="checkbox" name="homme" class="checkboxnews"> <p>J'ai lu et j'accepte la <a href="">politique de confidentialité</a>.</p>
                </div>    
                
                      
                
            </article>
        </section>
     

